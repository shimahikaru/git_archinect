<%= stylesheet_link_tag    'devise/user', media: 'all', 'data-turbolinks-track': 'reload' %>
<%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
<div class="usernew">
<h2>ログイン</h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <p><%= f.label :メールアドレス %></p>
    <%= f.email_field :email, autofocus: true, autocomplete: "email", class: 'user form' %>
  </div>

  <div class="field">
    <p><%= f.label :パスワード %></p>
    <%= f.password_field :password, autocomplete: "current-password", class: 'user form' %>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end -%>

  <div class="field">
    <%= f.submit "ログイン", class: 'user button' %>
  </div>
<% end %>
  <div class="field">
<%= render "devise/shared/links" %>
　　</div>
</div>
